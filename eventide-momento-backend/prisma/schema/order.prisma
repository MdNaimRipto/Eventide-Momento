model Order {
  id            String   @id @default(uuid())
  userId        String
  hostId        String
  eventId       String
  event         Event    @relation(fields: [eventId], references: [id])
  paidAmount    Int
  transectionId String   @unique
  paymentDate   DateTime?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
